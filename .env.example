APP_ENV=dev
APP_HTTP_ADDR=:8080
APP_METRICS_ADDR=:9090
DB_DSN=postgres://flagship:flagship@localhost:5432/flagship?sslmode=disable
ADMIN_API_KEY=admin-123
CLIENT_API_KEY=client-xyz
STORE_TYPE=postgres

# Rollout salt for deterministic user bucketing
# IMPORTANT: This should be a stable, random string that doesn't change across deployments
# Changing the salt will redistribute all users to different buckets!
# If not set, a random salt is generated on startup (not recommended for production)
ROLLOUT_SALT=your-stable-production-salt
